{
  "name": "adonuxt",
  "version": "0.0.1",
  "author": "",
  "description": "",
  "license": "MIT",
  "private": true,
  "main": "server.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node server.js",
    "serve:dev": "nodemon --watch app --watch bootstrap --watch config --watch .env -x node --inspect server.js",
    "build": "node ./ace nuxtbuild",
    "lint": "standard && eslint --ext .js,.vue resources/",
    "start": "echo \"\\033[31mWARN: 'start' will be depreciated soon. Instead use 'yarn serve'\" && yarn serve",
    "dev": "echo \"\\033[31mWARN: 'dev' will be depreciated soon. Instead use 'yarn serve:dev'\" && yarn serve:dev",
    "precommit": "yarn lint"
  },
  "dependencies": {
    "adonis-ace": "^3.0.6",
    "adonis-auth": "^1.0.5",
    "adonis-commands": "^2.1.5",
    "adonis-fold": "^3.0.3",
    "adonis-framework": "^3.0.9",
    "adonis-lucid": "^3.0.13",
    "adonis-middleware": "^1.0.10",
    "adonis-websocket": "^1.0.2",
    "adonis-websocket-client": "^1.0.2",
    "axios": "^0.16.0",
    "cross-env": "^3.1.4",
    "node-sass": "^4.5.2",
    "nuxt": "latest",
    "sass-loader": "^6.0.3",
    "sqlite3": "^3.1.8",
    "standard": "^8.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vuetify": "^0.10.3",
    "youch": "^2.0.4"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0"
  }
}
